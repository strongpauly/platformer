{
  "name": "platformer",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://paul.potsides.com/platformer",
  "dependencies": {
    "lodash": "^4.17.10",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-scripts-ts": "3.1.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "coverage": "react-scripts-ts test --env=jsdom --coverage",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "eject": "react-scripts-ts eject",
    "infrastructure": "terraform init ./infrastructure && terraform plan -out terraform.plan ./infrastructure && terraform apply terraform.plan"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^22.2.3",
    "@types/lodash": "^4.14.109",
    "@types/node": "^10.0.4",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "@types/react-redux": "^5.0.19",
    "@types/redux-logger": "^3.0.6",
    "coveralls": "^3.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "typescript": "^3.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/images.d.ts",
      "!**/src/index.tsx",
      "!**/src/registerServiceWorker.ts"
    ]
  }
}
